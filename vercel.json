{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.5.5" }
    },
    "routes": [{
        "src": "/(.*)",
        "dest": "/api/index.php"
    }],
    "redirects": [
        {
        "source": "/",
        "destination": "/public",
        "permanent": true
        }
    ],
    "rewrites": [
        {
        "source": "/(.*)",
        "destination": "/public/$1"
        }
    ]
}
